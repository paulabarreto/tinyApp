<!DOCTYPE html>
<html lang="en">
<header>
    <% include ./partials/_header %>
</header>
<body>
  <br><br>
  <button><a href="/urls/new">Create New</a></button>
  <br><br>
  <ul>
    <% for (var key in urls) { %>
      <li> <a href="/urls/<%= key %>"> <%= key %> : <%= urls[key] %></a>
        <form method="POST" action="/urls/<%= key %>?_method=DELETE"><button type="submit">Delete</button></form>
      </li>
    <% }; %>
  </ul>
</body>
